{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "prettier",
        "prettier/standard"
    ],
    "rules": {
        "prettier/prettier": "error",
        "quotes": "off",
        "no-console": "off"
    },
    "plugins": ["babel", "prettier"],
    "env": {
        "node": true
    }
}
